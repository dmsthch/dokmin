<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee"  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>dokmin</display-name>
  <servlet>
    <servlet-name>MController</servlet-name>
    <servlet-class>
		dokmin.library.controller.MController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MController</servlet-name>
    <url-pattern>*.member</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>BController</servlet-name>
    <servlet-class>
		dokmin.library.controller.BController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BController</servlet-name>
    <url-pattern>*.book</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>RController</servlet-name>
    <servlet-class>
		dokmin.library.controller.RController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RController</servlet-name>
    <url-pattern>*.rental</url-pattern>
  </servlet-mapping>
  
   <servlet>
    <servlet-name>LoginController</servlet-name>
    <servlet-class>
		dokmin.library.controller.LoginController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginController</servlet-name>
    <url-pattern>*.login</url-pattern>
  </servlet-mapping>
  
  
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>main.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
</web-app>